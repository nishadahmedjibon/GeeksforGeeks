
class Solution {
  public:
    int floorSqrt(int n) {
        long long low=1, high=n;
        while(low<=high)
        {
            long long mid=(low+high)/2;
            long long ans=mid*mid;
            if(ans<=(long long)(n))
            {
                low=mid+1;
            }
            else
            {
                high=mid-1;
            }
        }
        return high;
    }
};
