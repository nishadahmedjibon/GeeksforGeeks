#include<bits/stdc++.h>
using namespace std;

bool isPrime(int n){
    if(n<=1)
        return false;
    for(int i=2; i<sqrt(n); i++)
        if(n%i==0)
        return false;
    return true;
}
int main()
{
    int n;
    cin>>n;
    cout<<isPrime(n);
    return 0;
    /*The output 1 refers its prime
      The output 0 refers its not a prime
      */
}
